        android:layout_centerVertical="true"

        android:text="蚵仔炒麵70" />

    <Button
        android:id="@+id/btn8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn7"
        android:layout_centerVertical="true"

        android:text="蚵仔炒麵70" />
